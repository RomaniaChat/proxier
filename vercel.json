{
  "version": 2,
  "name": "proxier",
  "alias": [
    "proxier.now.sh"
  ],
  "routes": [
    {
      "src": "/(.*)",
      "methods": [
        "OPTIONS"
      ],
      "dest": "/api/200.js"
    },
    {
      "src": "/api",
      "methods": [
        "POST",
        "GET",
        "PUT",
        "PATCH"
      ]
    },
    {
      "src": "/(.*)",
      "dest": "/api?url=$1"
    }
  ]
}